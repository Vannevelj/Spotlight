{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "lib": [ "dom", "es6" ],
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "../wwwroot"
  ],
  "files": [
    "Index.ts",
    "Recorder.ts"
  ]
}

